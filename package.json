{
  "name": "@methodgrab/scroll-to",
  "version": "0.1.0",
  "description": "A wrapper around `jQuery.animate()` to scroll the page to a specific element",
  "license": "MIT",
  "repository": "MethodGrab/scroll-to",
  "author": "MethodGrab",
  "keywords": [
    "scroll",
    "scroll to"
  ],
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint *.js",
    "build": "browserify index.js --standalone scroll2 -t babelify --outfile dist/scroll-to.js"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "dist/scroll-to.js"
  ],
  "devDependencies": {
    "@methodgrab/eslint-config-standard": "^0.8.0",
    "ava": "^0.15.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^2.13.1",
    "jquery": "^3.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
}
